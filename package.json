{
	"name": "@puzzle-framework/react-app-flow",
	"version": "0.0.0",
	"description": "This is a React JS library to support and simplify navigations and maximize reuse screens in different contexts",
	"main": "dist/index.js",
	"module": "src/index.js",
	"files": [
		"dist/"
	],
	"scripts": {
		"build": "node scripts/pre-build.js && tsc",
		"start": "yarn run storybook",
		"storybook": "start-storybook -p 6006 -s public",
		"storybook:build": "build-storybook -s public",
		"storybook:serve": "yarn run storybook:build && npx http-server ./storybook-static/",
		"lint:errors": "eslint --quiet src/**/*.{ts,tsx}",
		"lint:fix": "eslint --fix src/**/*.{ts,tsx}",
		"lint:css": "stylelint ./src/**/*.{ts,tsx}",
		"prettier-format-all": "prettier --write src/**/*.{json,ts,tsx}",
		"lint:all": "yarn run lint:errors && yarn run spell:check && yarn run lint:css",
		"spell:check": "cspell src/**/*.{ts,tsx}",
		"jest": "jest --coverage --verbose --color",
		"test": "node scripts/test.js \"--watchAll=false\" \"--forceExit\"",
		"test:coverage": "node scripts/test.js \"--coverage\" \"--watchAll=false\" \"--silent\" \"--forceExit\"",
		"prepare": "husky install",
		"git:pre-commit": "lint-staged",
		"git:pre-push": "yarn run test:coverage"
	},
	"peerDependencies": {
		"react": "^17.0.0",
		"rxjs": "^6.6.2"
	},
	"devDependencies": {
		"@babel/core": "^7.11.4",
		"@babel/preset-react": "^7.10.4",
		"@commitlint/cli": "^13.2.0",
		"@commitlint/config-conventional": "^13.2.0",
		"@semantic-release/changelog": "^6.0.0",
		"@semantic-release/commit-analyzer": "^9.0.1",
		"@semantic-release/git": "^10.0.0",
		"@semantic-release/github": "^8.0.1",
		"@semantic-release/npm": "^8.0.0",
		"@semantic-release/release-notes-generator": "^10.0.2",
		"@storybook/addon-actions": "^6.0.20",
		"@storybook/addon-controls": "^6.0.21",
		"@storybook/addon-docs": "^6.0.20",
		"@storybook/addon-essentials": "^6.0.20",
		"@storybook/addon-knobs": "^6.0.20",
		"@storybook/addon-links": "^6.0.20",
		"@storybook/addon-viewport": "^6.0.20",
		"@storybook/node-logger": "^6.0.20",
		"@storybook/react": "^6.0.20",
		"@types/enzyme": "^3.10.7",
		"@types/jest": "^26.0.14",
		"@types/node": "^14.0.23",
		"@types/react": "^16.9.43",
		"@typescript-eslint/eslint-plugin": "^2.7.0",
		"@typescript-eslint/parser": "^2.7.0",
		"@wojtekmaj/enzyme-adapter-react-17": "^0.6.0",
		"anysort": "^2.0.0",
		"babel-eslint": "10.0.3",
		"babel-jest": "^26.3.0",
		"babel-loader": "^8.1.0",
		"copy-dir": "^1.3.0",
		"cspell": "^4.1.0",
		"cspell-dict-en-gb": "^1.1.27",
		"cspell-dict-en_us": "^1.2.37",
		"cspell-dict-lorem-ipsum": "^1.0.21",
		"enzyme": "^3.11.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-config-react-app": "^5.0.2",
		"eslint-loader": "3.0.0",
		"eslint-plugin-flowtype": "3.13.0",
		"eslint-plugin-import": "2.18.2",
		"eslint-plugin-jsx-a11y": "6.2.3",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-react": "7.14.3",
		"eslint-plugin-react-hooks": "^1.6.1",
		"eslint-plugin-sonarjs": "^0.5.0",
		"husky": "^7.0.2",
		"jest": "^26.4.2",
		"jest-canvas-mock": "^2.2.0",
		"jest-environment-jsdom-fourteen": "^1.0.1",
		"jest-junit": "^11.1.0",
		"jest-sonar-reporter": "^2.0.0",
		"jest-watch-typeahead": "^0.6.0",
		"lint-staged": "^11.1.2",
		"prettier": "^1.19.1",
		"react": "^17.0.0",
		"react-app-polyfill": "^1.0.6",
		"react-dom": "^17.0.0",
		"rimraf": "^3.0.2",
		"rxjs": "^7.3.0",
		"storybook-addon-i18n": "^5.1.13",
		"stylelint": "^13.6.1",
		"stylelint-config-recommended": "^3.0.0",
		"stylelint-config-styled-components": "^0.1.1",
		"stylelint-processor-styled-components": "^1.10.0",
		"ts-loader": "^8.0.3",
		"typescript": "^4.0.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/puzzle-framework/react-app-flow.git"
	},
	"author": {
		"email": "puzzlefraemwork@gmail.com",
		"name": "Puzzle Framework"
	}
}
